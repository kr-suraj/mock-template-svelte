{
	"name": "angelone",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"dev:debug": "NODE_OPTIONS=--inspect ./node_modules/.bin/vite dev --host",
		"dev:uat": "vite dev --host",
		"dev:cug": "vite dev --host",
		"dev:prod": "vite dev --host",
		"build": "vite build",
		"build:uat": "vite build --mode uat",
		"build:cug": "vite build --mode cug",
		"build:prod": "vite build --mode prod",
		"preview": "vite preview --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "eslint --cache .",
		"format": "prettier --plugin-search-dir . --cache --write .",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "2.0.0",
		"@sveltejs/adapter-node": "1.2.4",
		"@sveltejs/kit": "1.20.1",
		"@typescript-eslint/eslint-plugin": "5.59.9",
		"@typescript-eslint/parser": "5.59.9",
		"autoprefixer": "10.4.14",
		"eslint": "8.28.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-svelte": "2.30.0",
		"svelte-eslint-parser": "^0.32.2",
		"husky": "8.0.3",
		"postcss": "8.4.24",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"svelte": "3.59.1",
		"svelte-check": "3.4.3",
		"tailwindcss": "3.3.2",
		"tslib": "2.5.3",
		"typescript": "^5.0.0",
		"vite": "4.3.9",
		"vite-plugin-mkcert": "1.15.0",
		"vitest": "0.31.4"
	},
	"type": "module",
	"dependencies": {
		"daisyui": "^3.1.9",
		"fflate": "0.8.0",
		"firebase": "9.22.2",
		"svelte-components": "0.133.0",
		"svelte-library": "^0.12.0",
		"sveltekit-device-detector": "1.0.2",
		"ua-parser-js": "1.0.35"
	}
}
